package juegoiavj.kinematic.uniforme;

import juegoiavj.math.Vector;

/**
 *
 * @author Samuel
 */
public class  Steering {
    private Vector velocidad;
    private double rotacion;
    
    public Steering(){
        velocidad = new Vector();
        rotacion = 0;
    }
    
    public Steering(Vector v, double r){
        this.velocidad = v;
        this.rotacion = r;
    }
    
    public Vector getVel(){
        return velocidad;
    }
    
    public double getRot(){
        return rotacion;
    }
    
    public void setVel(Vector v){
        velocidad = v;
    }
    
    public void setRot(double r){
        rotacion = r;
    }
}
